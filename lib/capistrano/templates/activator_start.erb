nohup ./<%= fetch(:application) %> \
           -Dconfig.file=<%= activator_config_file_path %> \
           -Dlogger.file=<%= activator_logger_file_path %> \
           -Dpidfile.path=<%= activator_pid_path %> \
           -Dhttp.port=<%= fetch(:activator_http_port) %> \
           -Dhttp.address=<%= fetch(:activator_http_address) %> \
           -DapplyEvolutions.<%= fetch(:activator_db_name) %>=<%= fetch(:activator_apply_evolutions) %> \
           -J-server -mem <%= fetch(:activator_mem) %> <% fetch(:activator_jvm_args) %> > /dev/null 2>&1 & sleep 2
